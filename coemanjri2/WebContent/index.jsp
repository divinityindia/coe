<script type="text/javascript">
			$(function(){
			
				$.ajax({
	                type: "GET",
	                url: "loadevents",                
	                dataType: "json",
	                success:function(data){
	                	if(data){
	                		$.each(data, function(i,item) {
	                			var link1;
	                			var link2;
	                			if(item.eventId==0||item.documentLength==0)
	                				{
	                				 link1="#";
	                				}
	                			else
	                				{
	                					link1="loadevents?eventid="+item.eventId;
	                				}
	                			if(item.eventLink=="")
	                				{
	                					link2="#";
	                				}
	                			else
	                				{
	                					link2=""+item.eventLink;
	                				}
	                			$('#myEvents').append('<li class="clear"><p class="nospace btmspace-15" style="font-size: large;"><a style="color:orange;" href="'+link1+'">'+item.eventHeading+'</a></p><p><a href="'+link2+'">'+item.eventDetails+'</a></p></li>');
	                		  });
	                    }
	                	
	                	else
	                	{
	                		$('#myEvents').append('<li class="clear"><p class="nospace btmspace-15" style="font-size: large;"><a style="color:orange;" href="#">No events to show</a></p></li>');
	                	}
	                },
	                error:function(){
	                	
	                } 
	            });
	           
 			}); 
			
</script>

